ng.Replace(&quot;'&quot;,&quot;&quot;).Replace(&quot;|&quot;,&quot;&quot;)&#xD;&#xA;&#x9;strStartDate = row(4).ToString&#xD;&#xA;&#x9;strEndDate = row(5).ToString&#xD;&#xA;&#x9;strTotalTime = row(6).ToString&#xD;&#xA;&#x9;strOutcome = row(7).ToString&#xD;&#xA;&#x9;strOutcomeNotes = row(8).ToString.Replace(&quot;'&quot;,&quot;&quot;).Replace(&quot;|&quot;,&quot;&quot;)&#xD;&#xA;&#x9;strMiscNotes = row(9).ToString.Replace(&quot;'&quot;,&quot;&quot;).Replace(&quot;|&quot;,&quot;&quot;)&#xD;&#xA;&#x9;strExitPoint = row(10).ToString&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;'Add to insert statement string&#xD;&#xA;&#x9;'For the first row (including every 1000th where the insert string starts again) don't add a comma before the bracket (as this will be immediately after the VALUES statement&#xD;&#xA;&#x9;If intRowCount = 1 Or (intRowCount Mod 1000) = 0 Then&#xD;&#xA;&#x9;&#x9;strInsertStatement = strInsertStatement + &quot;(&quot; + strWorkPackageID + &quot;,&quot;&#xD;&#xA;&#x9;Else&#xD;&#xA;&#x9;&#x9;strInsertStatement = strInsertStatement + &quot;,(&quot; + strWorkPackageID + &quot;,&quot;&#xD;&#xA;&#x9;End If&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + strWorkPackageVersion + &quot;,&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;'&quot; + strUser + &quot;',&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;'&quot; + strTransactionID + &quot;',&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;convert(datetime,'&quot; + strStartDate + &quot;',101),&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;convert(datetime,'&quot; + strEndDate + &quot;',101),&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + strTotalTime + &quot;,&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;'&quot; + strOutcome + &quot;',&quot;&#xD;&#xA;&#x9;strInsertStatement = strInsertStatement + &quot;'&quot; + strOutcomeNotes + &quot;',&quot;&#xD;&#xA;&#x9;strInsertStatement = strIn